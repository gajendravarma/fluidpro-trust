# Environment variables template
# Copy this file to .env and fill in your actual values

# Wepsol customer configuration
WEPSOL_TENANT_ID=your-wepsol-tenant-id
WEPSOL_CLIENT_ID=your-wepsol-client-id
WEPSOL_CLIENT_SECRET=your-wepsol-client-secret

# CGL customer configuration
CGL_TENANT_ID=your-cgl-tenant-id
CGL_CLIENT_ID=your-cgl-client-id
CGL_CLIENT_SECRET=your-cgl-client-secret

# Legacy Office 365 settings (for backward compatibility)
AZURE_CLIENT_ID=your-azure-client-id
AZURE_CLIENT_SECRET=your-azure-client-secret
AZURE_TENANT_ID=your-azure-tenant-id

OFFICE365_CLIENT_ID=your-office365-client-id
OFFICE365_CLIENT_SECRET=your-office365-client-secret
OFFICE365_TENANT_ID=your-office365-tenant-id

# Other service configurations
DATTO_PUBLIC_KEY=your-datto-public-key
DATTO_SECRET_KEY=your-datto-secret-key
DATTO_BASE_URL=https://api.datto.com/v1

SITE24X7_ACCESS_TOKEN=your-site24x7-access-token
SITE24X7_REFRESH_TOKEN=your-site24x7-refresh-token
SITE24X7_API_DOMAIN=https://www.site24x7.in
ZOHO_CLIENT_ID=your-zoho-client-id
ZOHO_CLIENT_SECRET=your-zoho-client-secret
